<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jinhe.modules.system.dao.SysLoginMapper">


    <select id="selectUserByLogin" resultType="com.jinhe.modules.system.dto.SysLoginDto">
        SELECT id,state from sys_user where NORMALIZED_USERNAME = #{login.userName} And PASSWORD_HASH = #{login.passWord}
    </select>
</mapper>
